generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int      @id
  name         String
  email        String   @unique
  password     String
  access_token String   @unique
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
